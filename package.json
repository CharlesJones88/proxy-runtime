{
  "scripts": {
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "server": "ws --log.format dev",
    "docs": "typedoc --out ./docs assembly --ignoreCompilerErrors --name 'AssemblyScript Proxy Runtime'"
  },
  "devDependencies": {
    "assemblyscript": "^0.28.9",
    "http-server": "^0.12.3",
    "local-web-server": "^3.0.7",
    "minimist": ">=1.2.2",
    "typedoc": "^0.28.14"
  },
  "name": "@charlesjones88/proxy-runtime",
  "description": "Use this SDK to write extensions for the proxy WASM ABI",
  "version": "0.1.15",
  "main": "assembly/index.ts",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlesjones88/proxy-runtime.git"
  },
  "keywords": [
    "wasm",
    "proxy"
  ],
  "author": "Yuval Kohavi",
  "license": " Apache-2.0",
  "files": [
    "/assembly",
    "pnpm-lock.yaml",
    "index.js"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}
